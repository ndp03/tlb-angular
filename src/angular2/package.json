{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "scripts": {
    "start": "webpack-dev-server --config webpack_config/webpack.dev.js --progress --display-error-details",
    "build-local": "rimraf dist && webpack --config webpack_config/webpack.build-local.js --progress --profile --bail",
    "build": "rimraf dist && webpack --config webpack_config/webpack.prod.js --progress --profile --bail",
    "test": "karma start ./test/karma.config.js --reporters teamcity",
    "test-debug": "karma start ./test/karma.config.js --no-single-run --auto-watch",
    "karma": "karma start ./test/karma.config.js --reporters teamcity --single-run --browsers PhantomJS --colors false"
  },
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/router": "3.4.1",
    "angular2-moment": "^1.3.3",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.3",
    "karma": "^1.4.1",
    "rxjs": "5.1.1",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "2.5.35",
    "@types/jquery": "^2.0.34",
    "@types/lodash": "4.14.50",
    "@types/node": "^6.0.45",
    "angular2-template-loader": "^0.4.0",
    "autoprefixer": "^6.5.3",
    "awesome-typescript-loader": "^2.2.4",
    "css-loader": "^0.25.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.15.0",
    "import-glob-loader": "^1.1.0",
    "jasmine-core": "^2.5.2",
    "jquery": "^3.1.1",
    "karma": "^1.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-remap-istanbul": "^0.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-teamcity-reporter": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.17.2",
    "node-sass": "^3.11.1",
    "postcss-loader": "^1.1.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "typescript": "~2.0.6",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
